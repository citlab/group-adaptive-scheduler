            <name>flink</name>
            <arguments>
                <argument name="m">yarn-cluster</argument>
                <argument name="yn">12</argument>
            </arguments>
        </runner>
        <jar>
            <path>/data/vinh.tran/flink/examples/batch/KMeans.jar</path>
            <arguments>
