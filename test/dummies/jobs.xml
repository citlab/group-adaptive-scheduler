<?xml version="1.0" ?>
<jobs>
    <job name="kMeans">
        <runner>
            <name>flink</name>
            <arguments>
                <argument name="m">yarn-cluster</argument>
                <argument name="yn">3</argument>
            </arguments>
        </runner>
        <jar>
            <path>/data/vinh.tran/flink/examples/batch/KMeans.jar</path>
            <arguments>
                <argument name="--points">hdfs:///user/vinh.tran/kMeans/points</argument>
                <argument name="--centroids">hdfs:///user/vinh.tran/kMeans/centers</argument>
                <argument name="--iterations">3</argument>
            </arguments>
        </jar>
    </job>
    <job name="enumTriangles">
        <runner>
            <name>flink</name>
            <arguments>
                <argument name="m">yarn-cluster</argument>
                <argument name="yn">3</argument>
            </arguments>
        </runner>
        <jar>
            <path>/data/vinh.tran/flink/examples/batch/EnumTriangles.jar</path>
            <arguments>
            </arguments>
        </jar>
    </job>
    <job name="tpch10">
        <runner>
            <name>flink</name>
            <arguments>
                <argument name="m">yarn-cluster</argument>
                <argument name="yn">3</argument>
            </arguments>
        </runner>
        <jar>
            <path>/data/vinh.tran/jobData/TPCH10.jar</path>
            <arguments>
                <argument name="--customer">hdfs:///user/vinh.tran/tpch10small/customer.csv</argument>
                <argument name="--lineitem">hdfs:///user/vinh.tran/tpch10small/lineitem.csv</argument>
                <argument name="--nation">hdfs:///user/vinh.tran/tpch10small/nation.csv</argument>
                <argument name="--orders">hdfs:///user/vinh.tran/tpch10small/orders.csv</argument>
            </arguments>
        </jar>
    </job>

</jobs>